{% extends 'base.html.twig' %}

{% block title %}TypeProbleme index{% endblock %}

{% block body %}
    <h1>TypeProbleme index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Probleme</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_probleme in type_problemes %}
            <tr>
                <td>{{ type_probleme.id }}</td>
                <td>{{ type_probleme.probleme }}</td>
                <td>
                    <a href="{{ path('type_probleme_show', {'id': type_probleme.id}) }}">show</a>
                    <a href="{{ path('type_probleme_edit', {'id': type_probleme.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('type_probleme_new') }}">Create new</a>
{% endblock %}
